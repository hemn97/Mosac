<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.mosac_android">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:icon="@drawable/icon"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".activity.HomeActivity">
        </activity>
        <activity android:name=".activity.RegisterActivity">
        </activity>
        <activity android:name=".activity.PostDetailActivity">
        </activity>
        <activity android:name=".activity.PostCommentActivity">
        </activity>
        <activity android:name=".activity.TeamDetailActivity">
        </activity>
        <activity android:name=".activity.CreateTeamActivity">
        </activity>
        <activity android:name=".activity.CreatePostActivity">
        </activity>
    </application>
</manifest>
